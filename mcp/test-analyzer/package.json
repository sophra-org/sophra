{
  "name": "test-analyzer",
  "version": "1.0.0",
  "description": "MCP Test Analyzer Server",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "ts-node-dev src/index.ts",
    "cli": "ts-node src/cli.ts",
    "prisma:generate": "prisma generate --schema=../../prisma/test-analyzer.prisma",
    "prisma:migrate:dev": "prisma migrate dev --schema=../../prisma/test-analyzer.prisma",
    "prisma:migrate:deploy": "prisma migrate deploy --schema=../../prisma/test-analyzer.prisma",
    "prisma:migrate:reset": "prisma migrate reset --schema=../../prisma/test-analyzer.prisma --force",
    "prisma:studio": "prisma studio --schema=../../prisma/test-analyzer.prisma"
  },
  "dependencies": {
    "@prisma/client": "^5.22.0",
    "fastify": "^4.29.0",
    "glob": "^10.4.5",
    "inquirer": "^8.2.6",
    "ora": "^5.4.1",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@types/glob": "^8.1.0",
    "@types/inquirer": "^8.2.10",
    "@types/node": "^20.17.12",
    "@types/ora": "^3.2.0",
    "prisma": "^5.22.0",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.7.2"
  }
}
